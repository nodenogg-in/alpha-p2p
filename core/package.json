    "htmlparser2": "^9.1.0",
    "lib0": "^0.2.88",
    "nanoid": "^5.0.4",
    "starkdown": "^3.2.9",
    "superjson": "^2.2.1",
    "valibot": "^0.25.0",
    "y-protocols": "^1.0.6",
    "y-utility": "^0.1.3",
    "y-webrtc": "^10.3.0",
    "yjs": "^13.6.10"
  }
