{
  "name": "nodenoggin-core",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "require": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    },
    "./views/spatial": {
      "types": "./src/views/spatial/index.ts",
      "require": "./src/views/spatial/index.ts",
      "import": "./src/views/spatial/index.ts"
    },
    "./views": {
      "types": "./src/views/index.ts",
      "require": "./src/views/index.ts",
      "import": "./src/views/index.ts"
    },
    "./sync": {
      "types": "./src/sync/index.ts",
      "require": "./src/sync/index.ts",
      "import": "./src/sync/index.ts"
    },
    "./ui": {
      "types": "./src/ui/index.ts",
      "require": "./src/ui/index.ts",
      "import": "./src/ui/index.ts"
    }
  },
  "scripts": {
    "test:unit:watch": "vitest --watch",
    "test:unit": "vitest"
