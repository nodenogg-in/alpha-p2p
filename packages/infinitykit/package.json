{
  "name": "@figureland/infinitykit",
  "version": "0.0.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./minimap": {
      "types": "./dist/minimap.d.ts",
      "require": "./dist/minimap.js",
      "import": "./dist/minimap.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test:watch": "vitest --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "dev:lib": "NODE_ENV=development tsup",
    "build:lib": "tsup && pnpm build:lib:sourcemap",
    "build:lib:sourcemap": "tsc --emitDeclarationOnly --declaration",
    "size": "size-limit"
  },
  "devDependencies": {
    "@size-limit/file": "^11.1.2",
    "@size-limit/preset-small-lib": "^11.1.2",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "size-limit": "^11.1.2",
    "tsup": "^8.0.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.3",
    "@figureland/mathkit": "^0.0.35",
    "@figureland/statekit": "^0.0.98",
    "@figureland/toolkit": "^0.0.51",
    "@figureland/typekit": "^0.0.27"
  },
  "size-limit": [
    {
      "path": "dist/index.js"
    }
  ]
}
